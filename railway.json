{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksConfig": {
      "providers": ["python"],
      "phases": {
        "setup": {
          "commands": [
            "pip install -r backend/requirements.txt"
          ]
        },
        "start": {
          "cmd": "python backend/app.py"
        }
      }
    }
  }
}
